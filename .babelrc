{
    "presets": [
        "es2015",
        "stage-0",
        "react"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "components": "./src/components",
                    "containers": "./src/containers"
                }
            }
        ]
    ]
}